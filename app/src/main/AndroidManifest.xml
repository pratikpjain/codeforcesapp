<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.cfprofile">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/myicon"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/myicon"
        android:supportsRtl="true"
        android:theme="@style/Theme.MemeSharing">
        <activity android:name="com.example.cfprofile.latestSolved"></activity>
        <activity android:name="com.example.cfprofile.OneMore" />
        <activity android:name="com.example.cfprofile.difficulty" />
        <activity android:name="com.example.cfprofile.submission" />
        <activity android:name="com.example.cfprofile.DisplayInfo" />
        <activity android:name="com.example.cfprofile.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <meta-data
            android:name="preloaded_fonts"
            android:resource="@array/preloaded_fonts" />
    </application>

</manifest>